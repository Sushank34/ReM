            <ImageView android:id="@+id/imageView3" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginTop="15dp" android:src="@drawable/pranav" android:layout_alignParentTop="true" android:layout_centerHorizontal="true"/>
            <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:textSize="25dp" android:textColor="#282828" android:id="@+id/namedev" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="R.Pranav Ajay" android:layout_below="@+id/imageView3" android:layout_centerHorizontal="true"/>
            <TextView android:textAppearance="?android:attr/textAppearanceMedium" android:textSize="15dp" android:textColor="#878787" android:id="@+id/desig" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="CEO" android:layout_below="@+id/namedev" android:layout_centerHorizontal="true"/>
            <ImageView android:id="@+id/fbnic" android:layout_width="30dp" android:layout_height="30dp" android:src="@drawable/fb1" android:layout_toLeftOf="@+id/namedev" android:layout_alignTop="@+id/gitnic"/>
            <ImageView android:id="@+id/gitnic" android:layout_width="30dp" android:layout_height="30dp" android:layout_marginTop="17dp" android:src="@drawable/git" android:layout_below="@+id/desig" android:layout_alignLeft="@+id/gittanuj"/>
            <ImageView android:id="@+id/linnik" android:layout_width="35dp" android:layout_height="35dp" android:src="@drawable/in" android:layout_alignRight="@+id/textView1" android:layout_alignBottom="@+id/gitnic"/>
