<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Devs">

    <RelativeLayout android:orientation="vertical" android:id="@+id/header" android:background="#f75d59" android:layout_width="match_parent" android:layout_height="40dp" android:layout_alignParentLeft="true" android:layout_alignParentTop="true">
        <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:textSize="20dp" android:textStyle="bold" android:textColor="#ffffff" android:id="@+id/textView2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="50dp" android:text="Developers" android:layout_centerVertical="true"/>
        <ImageView android:id="@+id/imageView2" android:padding="5dp" android:layout_width="40dp" android:layout_height="wrap_content" android:layout_marginLeft="5dp" android:src="@drawable/backbtnmd"/>
        <TableRow android:id="@+id/tableRow1" android:background="#f75d59" android:layout_width="match_parent" android:layout_height="2dp" android:layout_alignParentBottom="true"/>
    </RelativeLayout>
    <ScrollView android:id="@+id/scrollView1" android:scrollbars="none" android:layout_width="match_parent" android:layout_height="wrap_content">
        <RelativeLayout android:orientation="vertical" android:id="@+id/RelativeLayout1" android:paddingBottom="10dp" android:layout_width="match_parent" android:layout_height="match_parent">
            <TableRow android:id="@+id/tableRow2" android:background="#e4e5e7" android:layout_width="match_parent" android:layout_height="2dp" android:layout_marginLeft="8dp" android:layout_marginTop="15dp" android:layout_marginRight="20dp" android:layout_below="@+id/fbnic" android:layout_alignParentLeft="true"/>
            <TableRow android:id="@+id/tableRow9" android:background="#e4e5e7" android:layout_width="match_parent" android:layout_height="2dp" android:layout_marginLeft="20dp" android:layout_marginTop="9dp" android:layout_marginRight="20dp" android:layout_below="@+id/fbtanuj" android:layout_alignParentLeft="true"/>
            <TableRow android:id="@+id/tableRow11" android:background="#e4e5e7" android:layout_width="match_parent" android:layout_height="2dp" android:layout_marginLeft="20dp" android:layout_marginTop="9dp" android:layout_marginRight="20dp" android:layout_below="@+id/fbtanuj2" android:layout_alignParentLeft="true"/>
            <TableRow android:id="@+id/tableRow13" android:background="#e4e5e7" android:layout_width="match_parent" android:layout_height="2dp" android:layout_marginLeft="20dp" android:layout_marginTop="9dp" android:layout_marginRight="20dp" android:layout_below="@+id/fbtanuj3" android:layout_alignParentLeft="true"/>
            <Button android:textSize="20dp" android:textStyle="bold" android:textColor="#ffffff" android:id="@+id/fbbutton" android:background="#3b5998" android:padding="10dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="5dp" android:layout_marginTop="15dp" android:layout_marginRight="5dp" android:text="Like us on facebook" android:layout_below="@+id/tableRow13" android:layout_alignParentLeft="true"/>
            <Button android:textSize="20dp" android:textStyle="bold" android:textColor="#ffffff" android:id="@+id/gmailbutton" android:background="#dd4b39" android:padding="10dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="5dp" android:layout_marginTop="10dp" android:layout_marginRight="5dp" android:text="mail us" android:layout_below="@+id/fbbutton" android:layout_alignParentLeft="true"/>
