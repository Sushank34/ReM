            <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:textSize="25dp" android:textColor="#282828" android:id="@+id/namedev1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="N.S.Sushank" android:layout_below="@+id/imageView78" android:layout_centerHorizontal="true"/>
            <TextView android:textAppearance="?android:attr/textAppearanceMedium" android:textSize="15dp" android:textColor="#878787" android:id="@+id/desigtanuj" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="HR" android:layout_below="@+id/namedev1" android:layout_centerHorizontal="true"/>
            <ImageView android:id="@+id/fbtanuj" android:layout_width="30dp" android:layout_height="30dp" android:layout_marginTop="21dp" android:src="@drawable/fb1" android:layout_below="@+id/desigtanuj" android:layout_alignLeft="@+id/textView1"/>
            <ImageView android:id="@+id/gittanuj" android:layout_width="30dp" android:layout_height="30dp" android:layout_marginRight="17dp" android:src="@drawable/git" android:layout_above="@+id/tableRow9" android:layout_alignRight="@+id/imageView78"/>
            <ImageView android:id="@+id/lintanuj" android:layout_width="35dp" android:layout_height="35dp" android:src="@drawable/in" android:layout_above="@+id/tableRow9" android:layout_alignLeft="@+id/linnik"/>
            <ImageView android:id="@+id/imageView88" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginTop="18dp" android:src="@drawable/somnath" android:layout_below="@+id/tableRow9" android:layout_alignLeft="@+id/imageView78"/>
            <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:textSize="25dp" android:textColor="#282828" android:id="@+id/namedev2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="P.Somnath" android:layout_below="@+id/imageView88" android:layout_centerHorizontal="true"/>
            <TextView android:textAppearance="?android:attr/textAppearanceMedium" android:textSize="15dp" android:textColor="#878787" android:id="@+id/desigtanuj2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Head Procurement" android:layout_below="@+id/namedev2" android:layout_centerHorizontal="true"/>
