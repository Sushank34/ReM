            <ImageView android:id="@+id/gittanuj2" android:layout_width="30dp" android:layout_height="30dp" android:layout_marginRight="17dp" android:src="@drawable/git" android:layout_above="@+id/tableRow11" android:layout_alignRight="@+id/imageView88"/>
            <ImageView android:id="@+id/lintanuj2" android:layout_width="35dp" android:layout_height="35dp" android:src="@drawable/in" android:layout_above="@+id/tableRow11" android:layout_alignLeft="@+id/linnik"/>
            <ImageView android:id="@+id/imageView98" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginTop="18dp" android:src="@drawable/subramanayam" android:layout_below="@+id/tableRow11" android:layout_alignLeft="@+id/imageView88"/>
            <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:textSize="25dp" android:textColor="#282828" android:id="@+id/namedev3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="K.D.V.Subramanyam" android:layout_below="@+id/imageView98" android:layout_centerHorizontal="true"/>
            <TextView android:textAppearance="?android:attr/textAppearanceMedium" android:textSize="15dp" android:textColor="#878787" android:id="@+id/desigtanuj3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Head Influencer" android:layout_below="@+id/namedev3" android:layout_centerHorizontal="true"/>
            <ImageView android:id="@+id/fbtanuj3" android:layout_width="30dp" android:layout_height="30dp" android:layout_marginTop="21dp" android:src="@drawable/fb1" android:layout_below="@+id/desigtanuj3" android:layout_alignLeft="@+id/textView1"/>
            <ImageView android:id="@+id/gittanuj3" android:layout_width="30dp" android:layout_height="30dp" android:layout_marginRight="17dp" android:src="@drawable/git" android:layout_above="@+id/tableRow13" android:layout_alignRight="@+id/imageView98"/>
            <ImageView android:id="@+id/lintanuj3" android:layout_width="35dp" android:layout_height="35dp" android:src="@drawable/in" android:layout_above="@+id/tableRow13" android:layout_alignLeft="@+id/linnik"/>
        </RelativeLayout>
    </ScrollView>


</LinearLayout>